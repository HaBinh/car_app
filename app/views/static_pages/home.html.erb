<div class="col-md-3">
  <h4>Tìm kiếm hãng xe</h4>
  <%= form_tag vehicles_search_path, :method => 'get'  do %>
    <%= text_field_tag :search, params[:search]%>
    <%= submit_tag "Search" %>
  <% end %>
  <%= link_to("Tìm kiếm nâng cao", new_search_path ) %>
</div>

<div class="col-md-9">
  <% if logged_in? %>
    <div class="row">
      <aside class="col-md-4">
        <section class="user_info">
          <%= render 'shared/user_info' %>
        </section>
      </aside>
      <div class="col-md-8">
        <h3>Lịch sử thuê xe</h3>
      </div>
    </div>
  <% else %>
  	<div class="center jumbotron">
  	  <h1> THUÊ XE CHẤT LƯỢNG, GIÁ CẢ HỢP LÝ </h1>

  	  <h2>ĐĂNG KÝ THÀNH VIÊN NGAY</h2>

  	  <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>
  	</div>
  <% end %>
</div>
